nmap -sV –p22 192.168.0.11
dmitry –pb 192.168.0.11
nikto –h http://192.168.0.11
telnet 192.168.0.11 21
curl –s –I 192.168.0.11
whatweb http://192.168.0.11

nmap --script smb-os-discovery -p 445 192.168.1.1/24


msf exploit(handler) > use auxiliary/scanner/smb/smb_version
msf auxiliary(smb_version) > set rhosts 192.168.1.1/24
rhosts => 192.168.1.1/24
msf auxiliary(smb_version) > run


